{
    "name": "thebachtiarz-company/laravel-base-v1",
    "description": "A Base Module for Laravel Project v1",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.40|^10.1",
        "nesbot/carbon": "^2.62"
    },
    "license": "MIT",
    "version": "1.0.9",
    "autoload": {
        "psr-4": {
            "TheBachtiarz\\Base\\": "src/"
        },
        "files": [
            "helper/helper.php"
        ]
    },
    "authors": [
        {
            "name": "thebachtiarz-company",
            "email": "thebachtiarz.company@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "thebachtiarz",
        "base"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TheBachtiarz\\Base\\ServiceProvider"
            ]
        }
    },
    "homepage": "https://github.com/thebachtiarz-company/laravel-base-v1",
    "support": {
        "issues": "https://github.com/thebachtiarz-company/laravel-base-v1/issues",
        "source": "https://github.com/thebachtiarz-company/laravel-base-v1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
